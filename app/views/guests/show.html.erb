<h1> Guest Page </h1>
<div id="Facts">
<h2>Name: <%= @guest.name %></h2>
</div>

<div id="Rooms">
<h3> Rooms </h3>
<% @rooms.each do |room| %>
<div id="<%= room.suite %>">
  <p>Suite: <%= room.suite %></p>
  <p>Rate: <%= room.rate %></p>
  <p>Hotel: <%= room.hotel.name %></p>
  <p>------------------------------</p>
</div>
<% end %>
</div>

